print_after: 10
display_after: 0
validate_after: 100
save_after: 0
max_iters: 10000000
dec_seq_length: 10
future_seq_length: 10
dec_conditional: true
future_conditional: true
binary_data: false
lstm {
  name: "lstm_1_enc"
  num_hid: 2048
  has_input: true
  input_dims: 3072
  w_dense {
    init_type: UNIFORM
    scale: 0.01
    epsilon: 0.0001
    momentum: 0.9
    l2_decay: 0.0001
    gradient_clip: 0.0001
    eps_decay_factor: 0.9
    eps_decay_after: 10000
  }
  w_diag {
    init_type: UNIFORM
    scale: 0.1
    epsilon: 0.0001
